{
  "name": "nextlevel-backend",
  "version": "0.0.7",
  "description": "back end 1 for nextlevel",
  "license": "MIT",
  "repository": "https://github.com/aretecode/nextlevel/tree/master/packages/back-end",
  "main": "dist/main.bundle.js",
  "keywords": [
    "monorepo",
    "nextlevel",
    "babel",
    "webpack",
    "backend"
  ],
  "engines": {
    "node": "6.9"
  },
  "scripts": {
    "sjs": "sjs",
    "serve-hot": "babel-node ./config/server.js",
    "serve-static": "node dist/main.bundle.js",
    "serve-watch": "node dist/main.bundle.js",
    "dist-watch": "webpack --watch --config config/webpack.config.dev.js;",
    "build": "NODE_ENV=development webpack --config config/webpack.config.prod.js",
    "buildd": "NODE_ENV=development webpack --config config/webpack.config.build.js",
    "buildp": "NODE_ENV=production webpack --config config/webpack.config.prod.js -p",
    "test": "NODE_ENV=testing mocha-webpack --webpack-config config/webpack.config.test.js \"test/**/*.js\"",
    "flow": "flow; test $? -eq 0 -o $? -eq 2"
  },
  "dependencies": {
    "nextlevel-helpers": "^0.0.4",
    "bluebird": "3.4.6",
    "xtpoints": "2.0.0",
    "body-parser": "1.3.1",
    "bottlejs": "1.5.0",
    "express": "4.14.0",
    "express-session": "1.11.3",
    "pouchdb": "6.0.7"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-external-helpers": "^6.18.0",
    "babel-plugin-syntax-exists": "0.0.3",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "6.18.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-flow-strip-types": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2015-webpack2": "^1.0.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-preset-stage-3": "6.17.0",
    "babel-runtime": "6.18.0",
    "beautify-benchmark": "^0.2.4",
    "benchmark": "^2.1.2",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "clean-webpack-plugin": "0.1.14",
    "connect-redis": "3.1.0",
    "flow-bin": "0.33.0",
    "json-loader": "^0.5.4",
    "loader-utils": "^0.2.16",
    "lodash": "4.17.2",
    "memdown": "1.2.4",
    "mocha": "3.1.2",
    "mocha-webpack": "0.7.0",
    "nodemon": "1.10.0",
    "sinon": "1.17.6",
    "sinon-chai": "2.8.0",
    "sweet-js-webpack-loader": "0.0.2",
    "sweet.js": "^2.2.1",
    "sweetjs-loader": "0.0.7",
    "uuid": "2.0.3",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "1.16.2",
    "webpack-hot-middleware": "2.13.2",
    "webpack-hot-server-middleware": "0.0.4"
  }
